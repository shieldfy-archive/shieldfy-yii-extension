{
    "name": "shieldfy/shieldfy-yii-extension",
    "type": "yii2-extension",
    "description": "This is the official PHP SDK for Shieldfy (shieldfy.io)",
    "keywords": [
        "shieldfy",
        "shieldfy-php-client",
        "shieldfy-yii-extension"
    ],
    "homepage": "https://github.com/shieldfy/shieldfy-yii-extension",
    "authors": [
        {
            "name": "Shieldfy Inc.",
            "email": "team@shieldfy.io",
            "homepage": "https://shieldfy.io"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "ext-curl": "*",
        "shieldfy/shieldfy-php-client":"*"
    },
    "autoload": {
        "psr-4": {
            "Shieldfy\\Extensions\\Yii\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
